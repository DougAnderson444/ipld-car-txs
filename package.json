		"@ipld/dag-pb": "^2.1.17",
		"dag-jose": "^2.0.1",
		"ipfs-core": "^0.15.4",
		"ipfs-core-config": "^0.4.1",
		"ipfs-core-utils": "^0.15.1",
		"ipfs-repo": "^15.0.2",
		"it-all": "^1.0.6",
		"mitt": "^3.0.0",
		"multiformats": "^9.7.1",
		"process": "^0.11.10",
		"util": "^0.12.4"
	}
}
